{
  "name": "@hasezoey/typegoose-v2",
  "version": "0.0.1",
  "description": "Mongoose reinterpreted",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=8.10.0"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ],
  "scripts": {
    "build": "npm run lint && tsc -p tsconfig.build.json",
    "buildtests": "npm run lint && tsc -p tsconfig.json",
    "watch": "tsc -w -p tsconfig.json",
    "lint": "tslint --project tsconfig.json",
    "test": "npm run buildtests && npm run mocha",
    "mocha": "nyc mocha \"./test/index.test.ts\" --timeout 15000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rimraf lib .tsbuildinfo .nyc_output coverage doc build",
    "doc": "typedoc --out ./doc ./src --mode modules --exclude \"**/internal/*\"",
    "doc:all": "typedoc --out ./doc ./src --mode modules",
    "prepare": "npm run build"
  },
  "repository": "github:hasezoey/typegoose-v2",
  "author": "hasezoey <hasezoey@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/bson": "^4.0.0",
    "@types/chai": "^4.2.0",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.3.1",
    "@types/node": "^8.10.51",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.6",
    "mocha": "^6.2.0",
    "mongodb-memory-server-global": "^5.2.0",
    "nyc": "^14.1.1",
    "rimraf": "*2.6.3",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.3.0",
    "tslint": "*5.18.0",
    "tslint-eslint-rules": "*5.4.0",
    "typedoc": "*0.15.0",
    "typescript": "*3.5.3"
  },
  "dependencies": {
    "bson": "^4.0.2",
    "loglevel": "^1.6.4",
    "mongodb": "^3.3.2",
    "mongodb-core": "^3.2.7",
    "object.fromentries": "^2.0.0",
    "reflect-metadata": "^0.1.13"
  }
}
