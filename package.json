{
  "name": "@hasezoey/typegoose-v2",
  "version": "0.0.1",
  "description": "Mongoose reinterpreted",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ],
  "scripts": {
    "build": "npm run lint && tsc -p tsconfig.build.json",
    "buildtests": "npm run lint && tsc -p tsconfig.json",
    "watch": "tsc -w -p tsconfig.json",
    "lint": "tslint --project tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverage",
    "test:watchCoverage": "jest --collectCoverage --watch",
    "clean": "rimraf lib .tsbuildinfo .nyc_output coverage doc build",
    "doc": "typedoc --out ./doc ./src --mode modules --exclude \"**/internal/*\"",
    "doc:all": "typedoc --out ./doc ./src --mode modules",
    "prepare": "npm run build"
  },
  "repository": "github:hasezoey/typegoose-v2",
  "author": "hasezoey <hasezoey@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "peerDependencies": {
    "class-validator": "^0.11.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/bson": "^4.0.0",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.146",
    "@types/mongodb": "^3.3.10",
    "@types/node": "^12.12.7",
    "class-validator": "^0.11.0",
    "coveralls": "^3.0.7",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.2",
    "nyc": "^14.1.1",
    "rimraf": "3.0.0",
    "source-map-support": "^0.5.16",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.0",
    "tslint": "5.20.1",
    "tslint-eslint-rules": "*5.4.0",
    "typedoc": "*0.15.0",
    "typescript": "3.7.2"
  },
  "dependencies": {
    "bson": "^4.0.2",
    "lodash": "^4.17.15",
    "loglevel": "^1.6.6",
    "mongodb": "^3.3.4",
    "mongodb-core": "^3.2.7",
    "object.fromentries": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^1.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "rootDir": "",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true,
        "disableSourceMapSupport": true
      }
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*{.ts}",
      "**/src/**",
      "!**/*.{d.ts,js}"
    ]
  }
}
